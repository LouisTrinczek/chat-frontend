    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-react": "^7.33.2",
    "lint-staged": "^15.2.2",
    "openapi-typescript-codegen": "^0.27.0",
